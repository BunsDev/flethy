{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "http configs",
  "extends": "./base.json",
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "target": "ESNext",
    "types": ["node"],
    "esModuleInterop": true,
    "declaration": true,
    "isolatedModules": false
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
